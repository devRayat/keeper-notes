{
  "name": "keeper-gql-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/server",
    "prestart": "cross-env NODE_ENV='production'",
    "watch": "nodemon ./bin/server --watch ./build/**/*.js --ignore ./src/*",
    "build": "tsc",
    "build:watch": "tsc -w",
    "build:prod": "cross-env NODE_ENV='production' tsc -w",
    "pretest": "cross-env NODE_ENV='test' knex migrate:up --env test",
    "test": "cross-env NODE_ENV=test jest --watchAll",
    "posttest": "cross-env NODE_ENV='test' knex migrate:down --env test",
    "predev": "cross-env NODE_ENV='development' concurrently \"yarn gql:gen\" \"knex migrate:up --env development\"",
    "dev": "cross-env NODE_ENV='development' concurrently \"yarn build:watch\" \"yarn watch\"",
    "gql:gen": "graphql-codegen",
    "gql:apollo:gen": "npx apollo codegen:generate --localSchemaFile=src/graphql/schema.graphql --target=typescript --tagName=gql",
    "migrate:latest": "knex migrate:latest",
    "migrate:up": "knex migrate:up",
    "migrate:down": "knex migrate:down"
  },
  "dependencies": {
    "@graphql-tools/utils": "^8.5.5",
    "apollo-datasource": "^3.3.0",
    "apollo-server-cache-redis": "^3.3.0",
    "apollo-server-express": "^3.5.0",
    "bcrypt": "^5.0.1",
    "connect-redis": "^6.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "datasource-sql": "^1.6.0",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "4.17",
    "express-session": "^1.17.2",
    "graphql": "15.8",
    "graphql-fields": "^2.0.3",
    "http-status-codes": "^2.1.4",
    "ioredis": "^4.28.2",
    "knex": "^0.95.14",
    "knex-tiny-logger": "^2.1.0",
    "morgan": "~1.9.1",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-resolvers": "^2.4.2",
    "@types/bcrypt": "^5.0.0",
    "@types/connect-redis": "^0.0.18",
    "@types/cookie-parser": "^1.4.2",
    "@types/express-session": "^1.17.4",
    "@types/faker": "^5.5.9",
    "@types/graphql-fields": "^1.3.4",
    "@types/ioredis": "^4.28.5",
    "@types/jest": "^27.0.3",
    "@types/mock-knex": "^0.4.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.1",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "altair-express-middleware": "^4.3.0",
    "apollo-server-integration-testing": "^3.0.0",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "faker": "^5.5.3",
    "graphql-playground-middleware-express": "^1.7.23",
    "jest": "^27.4.5",
    "knex-mock-client": "^1.6.1",
    "mock-knex": "^0.4.10",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
}
